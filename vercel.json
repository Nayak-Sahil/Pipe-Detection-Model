{
  "version": 2,
  "builds": [{
    "src": "vercel_startup.sh",
    "use": "@vercel/python",
    "config": { "maxLambdaSize": "50mb" }
  }],
  "routes": [{
    "src": "/static/(.*)",
    "headers": { "cache-control": "s-maxage=31536000, immutable" },
    "dest": "/static/$1"
  }, {
    "src": "/api/(.*)",
    "dest": "vercel_startup.sh"
  }]
}
